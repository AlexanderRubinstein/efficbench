    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Experiment",
            "type": "debugpy",
            "request": "launch",
            "program": "run_experiment.py",
            "args": [
                "--bench",
                // "mmlu",
                "mmlu_fields",
                "--split",
                "iid",
                "--iterations",
                "5",
                "--device",
                "cuda",
                "--skip_irt",
                // "--cache_path",
                // "cache/mmlu_fields_iid_5_cuda.pickle",
                "--num_workers",
                "4"
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "Generate Table Average",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/generate_table_avg.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Download Leaderboard",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/download_leaderboard.py",
            "args": [
                "--lb_type",
                // "mmlu_fields",
                "openllm_leaderboard",
                "--lb_savepath",
                // "data/leaderboard_fields_raw_22042025.pickle"
                "./data/leaderboard_raw_25042025.pickle"
            ],
            "console": "integratedTerminal"
        }
    ]
}
