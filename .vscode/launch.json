    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Experiment",
            "type": "debugpy",
            "request": "launch",
            "program": "run_experiment.py",
            "args": [
                "--bench",
                // "mmlu",
                "mmlu_fields",
                "--split",
                "iid",
                "--iterations",
                "5",
                "--device",
                "cuda",
                "--skip_irt",
                "--cache_path",
                // "cache/mmlu_fields_iid_5_cuda_02052025.pickle",
                "cache/debug.pickle",
                // "cache/mmlu_fields_iid_5_cuda_03052025_gsheet.pickle",
                "--num_workers",
                "4",
                "--sampling_names",
                "random,high-disagreement,low-disagreement",
                // "random,high-disagreement",
                "--filename_suffix",
                "_disagreement"
                // "_reproduced"
            ],
            "console": "integratedTerminal",
        },
        {
            "name": "Generate Table Average",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/generate_table_avg.py",
            "console": "integratedTerminal",
            "args": [
                "--filename_suffix_mmlu_fields",
                "_disagreement",
                // "",
                "--scenarios_to_skip",
                "harness_gsm8k_5"
            ],
        },
        {
            "name": "Download Leaderboard",
            "type": "debugpy",
            "request": "launch",
            "program": "scripts/download_leaderboard.py",
            "args": [
                "--lb_type",
                // "mmlu_fields",
                "openllm_leaderboard",
                "--lb_savepath",
                // "data/leaderboard_fields_raw_22042025.pickle"
                "./data/leaderboard_raw_25042025.pickle"
            ],
            "console": "integratedTerminal"
        }
    ]
}
